{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/chat",
      "dest": "./src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/socket.io/socket.io.js",
      "dest": "./src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/socket.io/",
      "dest": "./src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/socket.io/",
      "dest": "./src/index.js",
      "methods": ["POST"]
    }
  ]
}
